/*订单搜索*/
.orderSearch{
  .content{
    height: 100%;
    width: 100%;
    background-color:#f5f5f5;
    .sub-content{
      padding:0 16px;
      overflow:hidden;
      .content-header{
        margin:10px 0;
        color: #444;
        span{
          font-size:18px;
        }
        i{
          float:right;
          font-size:20px;
        }
      }
      .search-item{
        background: #fff;
        border: 1px solid #b2b2b2;
        border-radius: 6%;
        padding: 10px 35px;
        margin-right: 8px;
        float:left;
      }
    }
  }
}
