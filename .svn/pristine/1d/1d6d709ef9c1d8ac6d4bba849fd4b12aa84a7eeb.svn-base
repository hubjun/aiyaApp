/* 以下为地址列表 */
.addressDetail {
  background:#F3F3F3;
  .addr-background{
    background: #f3f5f5;
  }
  .padding{
    border-right: none;
    border-left: none;
  }
  .item-areaName{
    width: 225px;
    display: block;
    height: 34px;
    padding-top: 2px;
    line-height: 34px;
    /* vertical-align: middle; */
  }

  .input-error {
    color: #ff5f05 !important;
  }

  .input-error::-webkit-input-placeholder {
    color: #ff5f05 !important;
  }
  .setDefault{
    color:#252525;
    margin:20px 0px;
    .setTxt{
      text-indent: 10px;
    }
  }
  .delAddress{
    color:#CB291E;
  }
}

