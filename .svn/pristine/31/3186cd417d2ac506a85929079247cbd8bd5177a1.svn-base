  //若果获取返回的是error，则显示tis提示信息
  var tip = function(msg){
    $ionicLoading.show({
    template: msg,
    duration: 1500
    });
  };
  var page = {};

  $scope.toothPageParame = {
      keyword: '',
      salesDesc: '',
      pageNumber: 1,
      pageSize: 6,
      categoryIds: ''
