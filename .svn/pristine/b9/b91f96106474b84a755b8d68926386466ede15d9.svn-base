/* 以下为地址列表 */
.addressDetail {
  .addr-background{
    background: #f3f5f5;
  }
  .padding{
    border-right: none;
    border-left: none;
  }
  .item-areaName{
    display: block;
    height: 34px;
    padding-top: 2px;
    line-height: 34px;
    vertical-align: middle;
  }

  .input-error {
    color: #ff5f05 !important;
  }

  .input-error::-webkit-input-placeholder {
    color: #ff5f05 !important;
  }

}

