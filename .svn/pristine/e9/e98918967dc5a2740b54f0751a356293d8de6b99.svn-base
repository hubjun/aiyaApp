.tooth-op{
  background: #F4F4F4;
  .icon.ion-android-search.placeholder-icon {
    font-size:20px;
  }
  .slider-pager{
    bottom: 9px;
    .slider-pager-page.active{
      -webkit-transition: color .4s ease-in;
      transition:opacity .4s ease-in;
      color: #ff7f02;
    }
  }
  .slider{
    img{
      width: 100%;
      height: 6.5rem;
    }
  }
  .findNot {
    width: 3.98rem;
    height: 4rem;
    display: block;
    margin: auto;
    margin-top: 2rem;
  }
  .toogle-bar-wrap{

  .toogle-bar{
    width: 100%;
    height: 40px;
    margin: 10px 0 0;
    padding: 0 10px;
    background: #fff;
    display: box;
    display: -webkit-box;
    display: -moz-box;
    span{
      -webkit-box-flex: 1;
      display: block;
      text-align: center;
      color: #000000;
      font-size: 18px;
      line-height: 40px;
    }
    .toogleTabItemActive{
      color: #FF5F0F !important;
    }
    .toogleTabItemPri{
      display:block;
      float: left;
      color:#000000;
      width: 60%;
      text-align: right;
    }
    //向下向上字体图标
    .toogleTabArrow{
      display: block;
      padding-left:4px;
      height:40px;
      color: #000;
      float: left;
      i{
        display:block;
        font-size:12px;
        height:20px;

      }
      //箭头向上
      .toogleTabArrowUp{
        line-height: 35px;
      }
      //箭头向下
      .toogleTabArrowDown{
        line-height: 10px;
      }
    }
    .toogle-icon{

      width: 70px;
      margin: 10px 0;
      border-left: 1px solid #dfdfdf;
      text-align: center;
      i{
        color: #6D6D6D;
        font-size: 23px;
      }
    }
  }
    .toothBarPositionName{
      position:fixed;
      top:44px;
      left:0;
      z-index:1;
      border-bottom:1px solid #dedede;
    }
  }
  .toogle-bar-wrap-hide{
    display:none;
    margin-top:0!important;
    .toogle-bar-hide{
      margin-top:0!important;
    }
  }

  //商品网格、列表显示 start
  .pro{
    width:100%;
    overflow:hidden;
    a,span{
      display:block;
      text-decoration:none;
      color: #252525;
    }
    .pro-warp{
      background-color:#f3f5f7;
      width:100%;
      height:105px;
      border-top:1px solid #dfdfdf;
      border-bottom:1px solid #dfdfdf;
    }
    .pro-body{
      width:100%;
      overflow:hidden;
    }
    .pro-body-des{
      display:inline-block;
      position:absolute;
      left:8px;
      right:0;
      img{
        width:79px;
        height:79px;
        border:1px solid #dfdfdf;
        margin:10px 0 0 10px;
      }
      .pro-body-des-text{
        position:absolute;
        padding: 3px 0;
        font-size:12px;
        left:101px;
        right:10px;
        top:10px;
        bottom:10px;
        .list-name{
          .pro-name{
            font-size: 0.7rem;

          }
        }
        .fact-price{
          color: #808080;
          margin: 6px 0 0 0;
          font-size: 14px;
          text-decoration: line-through;
        }
        .dis-price{
          position: absolute;
          top: 43px;
          color: white;
          font-size: rem(24);
          line-height: 26px;
          background: #FF5F0F;
          border-radius: 6px;
          padding: 5px 12px;
        }
        .dis-price-single{
          color:#FF5F0F;
         font-size:14px;
          margin: 3px 0 0 0;
        }
        .pro-body-des-con{
          margin-top:5px;
        }
        .join-cart{
          display:block;
          position: absolute;
          padding: 6px 12px;
          font-size: 16px;
          top: 42px;
          right: 8px;
          cursor: pointer;
          //background: url("../img/joinCart.jpg") no-repeat;
          //background-size: 1.1rem 1.1rem;
          background: #ff5f0f;
          color: white;
          border-radius: 4px;
        }
      }
    }
  }
  .column-list{
    padding: 5px;
    .pro-row{
      float: left;
      //height: 236px;
      padding: 10px 3px;
      .list-con{
        border: 1px solid #dedede;
        background: #fff;
        .pro-item{
          width: 100%;
          height: 7.45rem;
          img{
            width: 100%;
            height: 7.45rem;
          }
        }
        .list-detail{
          padding: 7px 3px 3px;
          .list-name{
            height: 2rem;
            p.pro-name{
              color: #252525;
              font-size: 14px;
              margin: 0 0 3px;
              padding-left: 4px;
            }
          }
          .kind-price{
            position: relative;
            //padding: 10px 0 0 0;
            .fact-price{
              /*display: block;*/
              color: #808080;
              font-size: 14px;
              text-decoration: line-through;
            }
            .dis-price{
              line-height: 26px;
              color: #FF5F0F;
              font-size: 15px;
            }
            //.join-cart{
            //  position: absolute;
            //  width: 0.85rem;
            //  height: 0.85rem;
            //  top: 29px;
            //  right: 16px;
            //  cursor: pointer;
            //  background: url("../img/joinCart.jpg") no-repeat;
            //  background-size: 0.85rem 0.85rem;
            //}
          }
          .detail-enquiry{
            display: block;
            line-height: 26px;
            color: white;
            font-size: 15px;
            background: #FF5F0F;
            text-align: center;
            width:90%;
            height:30px;
            border-radius: 5px;
            margin: auto;
            margin-bottom:4px;
          }
        }
      }
    }
  }
  //商品网格、列表显示 end
  .noMore{
    font-size: rem(20);
    margin: 5px 0;
    text-align: center;
  }
  .bar-footer {
    height: 50px;
    border: none;
    padding: 0;
  }
}
//打开筛选，完成按钮
.popover-backdrop{
  .popover-wrapper{
    .complete-button{
      height:80%;
      margin-left:70%;
      margin-top:auto;
      margin-bottom:auto;
    }
  }
}
