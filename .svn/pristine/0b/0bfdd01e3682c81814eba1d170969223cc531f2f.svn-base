.brand .brand-left-btn{
  margin: 0 10px 0 0;
}
#sub_header_list .button.sub_button_select {
  border-bottom: 2px solid #FF5F0F;
  position: relative;
  color:#FF5F0F;
}
.brand .bar-subheader .scroll-view.scroll-x{
  overflow-x:hidden;
}
